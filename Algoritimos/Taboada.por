programa
{
	
	funcao inicio()
	{
		inteiro x , tab , rest
		escreva(" Entre com o valor da taboada: ")
		leia(tab)

		para (x = 1 ; x <= 10 ; x++)
		{
			rest = x * tab
			escreva("\n",x," X ",tab," = ",rest)
		}
		
	}
}
